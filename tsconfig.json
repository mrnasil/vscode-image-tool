{
    "compilerOptions": {
    "types": ["vscode"],
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./out",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules",
      ".vscode-test",
      "out"
    ]
  }
  